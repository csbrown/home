	excludesfile = ~/.config/git/excludes
[color]
	ui = true
[merge]
	tool = gvimdiff
[push]
	default = simple
[rebase]
	stat = true
[include]
	path = ~/.config/git/local/config
[rerere]
	enabled = 1
