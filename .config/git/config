[alias]
	b = branch -v
	br = branch
	br-c = branch --contains
	cm = commit
	co = checkout
	cp = cherry-pick
	dc = diff --cached
	graph = log --graph --decorate --oneline
	rb = rebase
	rb-c = rebase --continue
	root = rev-parse --show-toplevel
	st = status -s
